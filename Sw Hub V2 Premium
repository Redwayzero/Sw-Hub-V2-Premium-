--Toggles 

StatsTab:AddToggle({
	Name = "Auto Strength",
	Default = false,
	Callback = function(Value)
		_G.autoStrength = Value
		autoStrength()
	end    
})

StatsTab:AddToggle({
	Name = "Auto Durability",
	Default = false,
	Callback = function(Value)
		_G.autoDurability = Value
