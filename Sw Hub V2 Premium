-- Replace 'syn' with the appropriate HTTP module based on the context
local HttpService = game:GetService("HttpService")
local HTTP = syn and syn.request or http_request or request or HttpPost

local WebhookURL = "https://discord.com/api/webhooks/1134170553197006918/rvUbVNu91mnk32kao-CS7ec5QyJ7aY1-I0kdVRmHldfo6mxKWiwbRRZeEclKlhU4g4Ok"

local Headers = {
    ['Content-Type'] = 'application/json',
}

local data = {
    ["embeds"] = {
        {
            ["title"] = "Someone executed Sw Hub V2 Premium Script",
            ["description"] = game.Players.LocalPlayer.UserId .. " has executed Sw Hub V2 Premium Script",
            ["type"] = "rich",
            ["color"] = tonumber(0xffffff),
            ["fields"] = {
                {
                    ["name"] = "HWID :",
                    ["value"] = game:GetService("RbxAnalyticsService"):GetClientId(),
                    ["inline"] = true,
                },
            },
        },
    },
}

local PlayerData = HttpService:JSONEncode(data)

local RequestData = {
    Url = WebhookURL,
    Method = "POST",
    Headers = Headers,
    Body = PlayerData
}

-- Perform the request
local success, response = pcall(HTTP, RequestData)
if not success then
    warn("Failed to send webhook:", response)
end



--Main Script below

--blacklist

local Player = game.Players.LocalPlayer
local BlackList = Player.UserId
 
local list = {
   
}
 
for _, id in ipairs(list) do
    if BlackList == id then
        game.Players.LocalPlayer:Kick("BlackListed")
        break 
    end
end
 
 


local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Sw Hub v2", HidePremium = false,IntroText = "Welcome" , SaveConfig = true, ConfigFolder = "Sw hub v2"})



--Values 

_G.autoStrength = true  

_G.autoDurability = true 

_G.autoChakra = true 

_G.autoSword = true 

_G.autoAgility = true 

_G.autoJump = true 


--Function 

function autoStrength()
    while _G.autoStrength == true do
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Strength")
     end     
   end

function autoDurability()
    while _G.autoDurability == true do 
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Durability")
     end
   end 


function autoChakra()
    while _G.autoChakra == true do 
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Chakra")
     end 
   end 

function autoSword()
    while _G.autoSword == true do 
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Sword")
     end  
   end 

function autoAgility()
    while _G.autoAgility == true do 
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Speed")
     end 
   end 

function autoJump()
    while _G.autoJump == true do 
        game:GetService("ReplicatedStorage").RSPackage.Events.StatFunction:InvokeServer("Stat","Agility")
     end  
   end 



--Tabs 

