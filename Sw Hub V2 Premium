--Buttons 

OpTab:AddButton({
	Name = "Faster Strength",
	Callback = function()
      		local replicatedStorage = game:GetService("ReplicatedStorage")
local events = replicatedStorage:WaitForChild("RSPackage"):WaitForChild("Events")

local function invokeServerFunction(stat, value)
    events.StatFunction:InvokeServer(stat, value)
end

local stat = "Stat"
local value = "Strength"

while true do
    invokeServerFunction(stat, value)
end
	end
})



